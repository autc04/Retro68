# Select only 680x0-specific CPUs.

M68K_MLIB_CPU += && (CPU !~ "^mcf")
M68K_ARCH := m68k

m68k-mac-pragmas.o: $(srcdir)/config/m68k/m68k-mac-pragmas.c
	  $(COMPILE) $<
	  $(POSTCOMPILE)

M68K_MLIB_CPU += && (CPU !~ "^m68060") && (CPU !~ "^cpu32") && (CPU !~ "^fidoa")

PASSES_EXTRA += $(srcdir)/config/m68k/m68k-passes.def
