     _Interface_:   'function omp_init_allocator(memspace, ntraits, traits)'
                    'integer (omp_allocator_handle_kind) ::
                    omp_init_allocator'
                    'integer (omp_memspace_handle_kind), intent(in) ::
                    memspace'
                    'integer, intent(in) :: ntraits'
                    'type (omp_alloctrait), intent(in) :: traits(*)'

