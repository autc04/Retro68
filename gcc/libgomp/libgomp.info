'fr_name'      'const char *'            str            '"hsa"'
'vendor'       'int'                     int            '1'
'vendor_name'  'const char *'            str            '"amd"'
'device_num'   'int'                     int
'platform'     N/A
'device'       'hsa_agent *'             ptr
